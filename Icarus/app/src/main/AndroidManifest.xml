<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alejandrocaralt.icarus">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icarus"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icarus"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activitys.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.MainActivity" />
        <activity android:name=".Activitys.RegisterActivity" />
        <activity android:name=".Activitys.EventDetailActivity" />
        <activity android:name=".Activitys.EventPresentActivity" />
        <activity android:name=".Activitys.EventCreateActivity"></activity>
    </application>

</manifest>